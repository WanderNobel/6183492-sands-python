   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def generate_sine_wave(frequency, duration, sample_rate):\n",
    "    samples = duration * sample_rate \n",
    "    t = np.linspace(0, duration, samples)\n",
    "    wave = np.sin(2 * np.pi * frequency * t)\n",
    "    return wave \n",
    "\n",
    "plt"
   ]
  }
